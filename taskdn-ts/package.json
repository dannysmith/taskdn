{
  "name": "@taskdn/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Taskdn - bindings to the Rust library",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "napi": {
    "binaryName": "taskdn",
    "targets": [
      "aarch64-apple-darwin",
      "x86_64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "x86_64-pc-windows-msvc"
    ]
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "bun test",
    "version": "napi version",
    "task:complete": "node scripts/complete-task.js",
    "task:rename-done": "node scripts/complete-task.js --rename-existing"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.0.0-alpha.62"
  },
  "engines": {
    "node": ">= 18"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dannysmith/taskdn"
  }
}
